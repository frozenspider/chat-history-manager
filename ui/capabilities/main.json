{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Capability for all windows",
  "windows": [
    "*"
  ],
  "permissions": [
    "app:default",
    "path:default",
    "event:default",
    "window:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-open",
    "dialog:default"
  ]
}
