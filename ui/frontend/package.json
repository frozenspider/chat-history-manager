{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "copy-scalapb": "shx cp ../../scalapb/_scalapb.proto ../../scalapb/scalapb.proto",
    "protobuf": "pnpm run copy-scalapb && shx mkdir -p src/protobuf && pnpm run protobuf-protoc-inner",
    "protobuf-protoc-inner": "run-script-os",
    "protobuf-protoc-inner:windows": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=src/protobuf/ --ts_proto_opt=env=browser,esModuleInterop=true,outputServices=nice-grpc,outputServices=generic-definitions,outputPartialMethods=false,forceLong=bigint,removeEnumPrefix=true,emitImportedFiles=false,oneof=unions,enumsAsLiterals=true,noDefaultsForOptionals=true -I ../.. core/protobuf/entities.proto backend/protobuf/services.proto",
    "protobuf-protoc-inner:default": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto        --ts_proto_out=src/protobuf/ --ts_proto_opt=env=browser,esModuleInterop=true,outputServices=nice-grpc,outputServices=generic-definitions,outputPartialMethods=false,forceLong=bigint,removeEnumPrefix=true,emitImportedFiles=false,oneof=unions,enumsAsLiterals=true,noDefaultsForOptionals=true -I ../.. core/protobuf/entities.proto backend/protobuf/services.proto",
    "build": "pnpm run protobuf && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.1.1",
    "@tauri-apps/plugin-dialog": "^2.2.0",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "next": "15.1.3",
    "protobufjs": "^7.4.0",
    "nice-grpc-common": "^2.0.2",
    "nice-grpc-web": "^3.3.5",
    "long": "^5.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "react-intersection-observer": "9.14.1",
    "react-resizable-panels": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-checkbox": "^1.1.3",
    "react-day-picker": "^8.10.1",
    "date-fns": "^3.6.0",
    "ogv": "^1.8.9",
    "camelcase-keys-deep": "^0.1.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/camelcase-keys-deep": "^0.1.2",
    "@tauri-apps/cli": "^2.1.0",
    "shx": "^0.3.4",
    "run-script-os": "^1.1.6",
    "copy-webpack-plugin": "^12.0.2",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "ts-proto": "^1.170.0",
    "react-devtools": "^5.0.2"
  },
  "packageManager": "pnpm@8.15.5+sha1.a58c038faac410c947dbdb93eb30994037d0fce2"
}
