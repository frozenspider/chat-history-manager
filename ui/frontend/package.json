{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "copy-scalapb": "cp ../../scalapb/_scalapb.proto ../../scalapb/scalapb.proto",
    "protobuf": "pnpm run copy-scalapb && mkdir -p src/protobuf && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/protobuf/ --ts_proto_opt=env=browser,esModuleInterop=true,outputServices=nice-grpc,outputServices=generic-definitions,outputPartialMethods=false,forceLong=bigint,removeEnumPrefix=true,emitImportedFiles=false,oneof=unions,snakeToCamel=false,enumsAsLiterals=true -I ../.. core/protobuf/entities.proto backend/protobuf/services.proto",
    "build": "pnpm run protobuf && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@tauri-apps/api": "2.0.0-beta.6",
    "@tauri-apps/plugin-fs": "2.0.0-beta.2",
    "react": "^18",
    "react-dom": "^18",
    "next": "14.1.4",
    "protobufjs": "^7.2.6",
    "nice-grpc-common": "^2.0.2",
    "nice-grpc-web": "^3.3.3",
    "long": "^5.2.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.363.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "react-resizable-panels": "^2.0.16",
    "@radix-ui/react-scroll-area": "^1.0.5"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "ts-proto": "^1.170.0",
    "react-devtools": "^5.0.2"
  },
  "packageManager": "pnpm@8.15.5+sha1.a58c038faac410c947dbdb93eb30994037d0fce2"
}
